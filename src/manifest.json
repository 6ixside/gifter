{
 "manifest_version": 2,
 "name": "gifter",
 "description": "gifter application as chrome extension using angular",
 "version": "1.0",
 "browser_action": {
 "default_icon": "assets/icon.png",
 "default_popup": "index.html"
 },
 "permissions": [
 	"storage"
 ],
 "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
 "web_accessible_resources": [
 	"assets/*.abi",
 	"assets/*.png",
 	"assets/fonts/*.woff"
 ]
}